    "@react-three/postprocessing": "^2.0.5",
    "@tailwindcss/forms": "^0.3.4",
    "@tailwindcss/typography": "^0.4.0",
    "autoprefixer": "^10.4.0",
    "esbuild": "^0.12.29",
    "github-slugger": "^1.4.0",
    "gray-matter": "^4.0.2",
    "image-size": "1.0.0",
    "mdx-bundler": "^6.0.3",
    "next": "12.0.3",
    "next-themes": "^0.0.15",
    "next-translate": "^1.2.0",
    "postcss": "^8.3.11",
    "preact": "^10.5.15",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "reading-time": "1.5.0",
    "rehype-autolink-headings": "^6.1.0",
