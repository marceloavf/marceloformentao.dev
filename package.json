    "rehype-prism-plus": "^1.1.1",
    "rehype-slug": "^5.0.0",
    "remark-footnotes": "^4.0.1",
    "remark-gfm": "^2.0.0",
    "remark-math": "^5.1.0",
    "sharp": "^0.29.2",
    "smoothscroll-polyfill": "^0.4.4",
    "tailwindcss": "^2.2.19",
    "three": "^0.134.0",
    "unist-util-visit": "^4.1.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "12.0.3",
    "@next/eslint-plugin-next": "^12.0.3",
    "@svgr/webpack": "^6.0.0-alpha.4",
    "cross-env": "^7.0.3",
    "dedent": "^0.7.0",
    "eslint": "^7.23.0",
    "eslint-config-next": "12.0.3",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "file-loader": "^6.0.0",
    "globby": "11.0.3",
    "husky": "^7.0.4",
    "inquirer": "^8.2.0",
    "lint-staged": "^12.0.0",
    "next-remote-watch": "^1.0.0",
    "prettier": "2.4.1"
  },
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=16.0.0"
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "eslint --fix"
    ],
    "*.+(js|jsx|ts|tsx|json|css|md|mdx)": [
      "prettier --write"
    ]
  }
}
